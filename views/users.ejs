<h3>My users IDs are: </h3>

<ul>
  <% if (typeof usersArray !== 'undefined' && usersArray.length > 0) { %>
    <% for (var i = 0; i < usersArray.length; i++) { %>
      
        <li>ID: <%= usersArray[i]._id %>
        
        UserName: <%= usersArray[i].username %>
       <%= usersArray[i].firstName %></li>
      <% } %>
  <% } %>
</ul>